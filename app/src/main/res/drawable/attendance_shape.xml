<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <stroke
                android:width="2dp"
                android:color="#297FCA" />
        </shape>
    </item>

    <item
        android:bottom="2dp"
        android:right="2dp"
        >
        <shape android:shape="rectangle">
            <stroke
                android:width="2dp"
                android:color="#F7FBFD" />
        </shape>
    </item>
</layer-list>